{
  "name": "tsx-fullstack-app-template",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Typescript, Node.js, Express and Webpack.",
  "main": "src/server/index.ts",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && ts-node src/server/index.ts",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Mark Poussard",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "express": "^4.16.3",
    "knex": "^0.20.7",
    "mysql": "^2.17.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.7.7",
    "@types/express": "^4.17.2",
    "@types/knex": "^0.16.1",
    "@types/mysql": "^2.15.8",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.9",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^5.1.3",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.17.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.23.0",
    "ts-node": "^8.6.1",
    "typescript": "^3.7.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
